<Virtualhost *:80>
	  DocumentRoot "/var/www/kemper"
	  ServerName "localhost"
	  ProxyPreserveHost On
	  ProxyRequests off
	  ProxyPass        "/"  "http://<%= @address %>:8080/<%= @appname %>/simplejsp.jsp" keepalive=on
	  ProxyPassReverse "/"  "http://<%= @address %>:8080/<%= @appname %>/simplejsp.jsp"
</Virtualhost>